{% extends 'site/area/page.html.twig' %}

{% block pageTitle %}History of changes of {{ area.title }} - {% endblock %} 



{% block htmlHeader %}
	<META NAME="ROBOTS" CONTENT="NOINDEX, NOFOLLOW">
{% endblock %}

{% block tabHistory %}currentTab{% endblock %}

{% block innerContent %}

	<ul class="history">
		{% for historyItem in historyItems %}
			{%  if historyItem.siteWebTemplate %}
				{%  include historyItem.siteWebTemplate with { 'historyItem':historyItem } %}
			{% endif %}
		{% endfor %}
	</ul>

{% endblock %}


