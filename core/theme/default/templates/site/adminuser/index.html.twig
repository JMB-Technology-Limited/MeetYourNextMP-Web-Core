{% extends 'site/page.html.twig' %}


{% block pageTitle %}User Admin - {% endblock %}


{% block breadcrumbs %}
	{% if not config.isSingleSiteMode %}<li><a href="/">{{ currentSite.title }}</a></li>{% endif %}
	<li><a href="/admin">admin</a></li>
	<li class="current"><a href="/admin/user">users</a></li>
{% endblock %}

{% block content %}

	<h2>User {{ user.username }}</h2>


	<h3>Has User Permissions</h3>

	<table class="data">
		<tr>
			<th>Extension</th>
			<th>ID</th>
		</tr>
		{% for userpermission in userpermissions %}
			<tr>
				<td>{{ userpermission.userPermissionExtensionID }}</td>
				<td>{{ userpermission.userPermissionKey }}</td>
			</tr>
		{% endfor %}
	</table>




		
{% endblock %}
